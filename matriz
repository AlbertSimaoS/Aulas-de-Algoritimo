#include <stdio.h>

int main()
{
    int mat[3][3];
    int j, k;
    printf("\t\Matrizes\n\n");
    for (j = 0; j < 3; j++)
        for (k = 0; k < 3; k++)
            mat[j][k] = 99;
        for (j = 0; j < 3; j++)
            for (k = 0; k < 3; k++)
                for(k = 0; k < 3; k++)
                    {
                        printf("%d", mat[j][k]);
                        if (k == 2)
                        printf("\n");
                    }
    return 0;
}
